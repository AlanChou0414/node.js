<%- include('parts/html-head') %> 
<%- include('parts/navbar') %> 

<div class="container">
    <table class="table table-bordered table-striped table-hover">
        <thead>
          <tr>
            <th scope="col">員工編號</th>
            <th scope="col">姓名</th>
            <th scope="col">年齡</th>
          </tr>
        </thead>
        <tbody>
            <!-- use for of to import json-sales.ejs -->
            <% for(let s of data) { %> 
          <tr>
            <td><%= s.id %> </td>
            <td><%= s.name %> </td>
            <td><%= s.age %> </td>
          </tr>
          <% } %> 
        </tbody>
      </table>
</div>

<%- include('parts/script') %> 
